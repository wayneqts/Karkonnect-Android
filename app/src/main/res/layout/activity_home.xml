<?xml version="1.0" encoding="utf-8"?>
<androidx.drawerlayout.widget.DrawerLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".activities.Home"
    tools:openDrawer="start"
    android:background="@color/white"
    android:id="@+id/drawer"
    >
    <RelativeLayout
        android:fitsSystemWindows="true"
        android:layout_width="match_parent"
        android:layout_height="match_parent">
        <RelativeLayout
            android:paddingTop="@dimen/dp_10"
            android:paddingHorizontal="@dimen/dp_10"
            android:id="@+id/rl_top_bar"
            android:layout_width="match_parent"
            android:layout_height="wrap_content">
            <androidx.appcompat.widget.Toolbar
                android:id="@+id/tool_bar"
                android:layout_width="@dimen/dp_35"
                android:layout_height="@dimen/dp_35"
                android:visibility="gone" />
            <ImageView
                android:layout_centerVertical="true"
                android:id="@+id/bt_toolbar"
                android:src="@mipmap/ic_menu"
                android:layout_width="@dimen/dp_30"
                android:layout_height="@dimen/dp_30"/>
            <TextView
                android:visibility="gone"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_centerInParent="true"
                android:textSize="@dimen/sp_22"
                android:textColor="@color/color_primary"
                android:fontFamily="@font/montserrat_semibold"
                android:id="@+id/tv_title"/>
            <ImageView
                android:id="@+id/iv_home"
                android:layout_centerHorizontal="true"
                android:layout_width="@dimen/dp_125"
                android:layout_height="@dimen/dp_50"
                android:src="@drawable/img_logo"/>
            <ImageView
                android:visibility="gone"
                android:layout_alignParentEnd="true"
                android:layout_centerVertical="true"
                android:id="@+id/bt_add"
                android:src="@mipmap/ic_add"
                android:layout_width="@dimen/dp_30"
                android:layout_height="@dimen/dp_30"/>
        </RelativeLayout>
        <FrameLayout
            android:id="@+id/fl_main"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:layout_below="@id/rl_top_bar"/>
    </RelativeLayout>
    <RelativeLayout
        android:fitsSystemWindows="true"
        android:id="@+id/rl_menu"
        android:layout_width="wrap_content"
        android:layout_height="match_parent"
        android:layout_gravity="start"
        android:background="@color/white"
        android:clickable="true">
        <ImageView
            android:id="@+id/iv_logo"
            android:layout_marginTop="@dimen/dp_20"
            android:layout_centerHorizontal="true"
            android:layout_width="@dimen/dp_200"
            android:layout_height="@dimen/dp_55"
            android:src="@drawable/img_logo"/>
        <androidx.core.widget.NestedScrollView
            android:layout_marginTop="@dimen/dp_20"
            android:layout_below="@id/iv_logo"
            android:layout_width="match_parent"
            android:layout_height="wrap_content">
            <androidx.appcompat.widget.LinearLayoutCompat
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:orientation="vertical">
                <androidx.appcompat.widget.LinearLayoutCompat
                    android:layout_marginHorizontal="@dimen/dp_20"
                    android:id="@+id/mn_home"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:gravity="center_vertical">
                    <ImageView
                        app:tint="@color/black"
                        android:src="@mipmap/ic_home"
                        android:layout_width="@dimen/dp_20"
                        android:layout_height="@dimen/dp_20"/>
                    <TextView
                        android:layout_marginTop="@dimen/dp_2"
                        android:textAllCaps="true"
                        android:text="@string/home"
                        android:layout_marginStart="@dimen/dp_10"
                        android:fontFamily="@font/montserrat_semibold"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:textSize="@dimen/sp_16"
                        android:textColor="@color/black"/>
                </androidx.appcompat.widget.LinearLayoutCompat>
                <androidx.appcompat.widget.LinearLayoutCompat
                    android:layout_marginHorizontal="@dimen/dp_20"
                    android:layout_marginTop="@dimen/dp_20"
                    android:id="@+id/mn_search"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:gravity="center_vertical">
                    <ImageView
                        app:tint="@color/black"
                        android:src="@mipmap/ic_search"
                        android:layout_width="@dimen/dp_20"
                        android:layout_height="@dimen/dp_20"/>
                    <TextView
                        android:layout_marginTop="@dimen/dp_2"
                        android:textAllCaps="true"
                        android:text="@string/search_title"
                        android:layout_marginStart="@dimen/dp_10"
                        android:fontFamily="@font/montserrat_semibold"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:textSize="@dimen/sp_16"
                        android:textColor="@color/black"/>
                </androidx.appcompat.widget.LinearLayoutCompat>
                <androidx.appcompat.widget.LinearLayoutCompat
                    android:layout_marginHorizontal="@dimen/dp_20"
                    android:layout_marginTop="@dimen/dp_20"
                    android:id="@+id/mn_inquire"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:gravity="center_vertical">
                    <ImageView
                        app:tint="@color/black"
                        android:src="@drawable/ic_inquire"
                        android:layout_width="@dimen/dp_20"
                        android:layout_height="@dimen/dp_20"/>
                    <TextView
                        android:layout_marginTop="@dimen/dp_2"
                        android:textAllCaps="true"
                        android:text="@string/inquire_rent_buy"
                        android:layout_marginStart="@dimen/dp_10"
                        android:fontFamily="@font/montserrat_semibold"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:textSize="@dimen/sp_16"
                        android:textColor="@color/black"/>
                </androidx.appcompat.widget.LinearLayoutCompat>
                <androidx.appcompat.widget.LinearLayoutCompat
                    android:layout_marginHorizontal="@dimen/dp_20"
                    android:layout_marginTop="@dimen/dp_20"
                    android:id="@+id/mn_pf"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:gravity="center_vertical">
                    <ImageView
                        app:tint="@color/black"
                        android:src="@mipmap/ic_pf"
                        android:layout_width="@dimen/dp_20"
                        android:layout_height="@dimen/dp_20"/>
                    <TextView
                        android:layout_marginTop="@dimen/dp_2"
                        android:textAllCaps="true"
                        android:text="@string/my_profile"
                        android:layout_marginStart="@dimen/dp_10"
                        android:fontFamily="@font/montserrat_semibold"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:textSize="@dimen/sp_16"
                        android:textColor="@color/black"/>
                </androidx.appcompat.widget.LinearLayoutCompat>
                <androidx.appcompat.widget.LinearLayoutCompat
                    android:layout_marginHorizontal="@dimen/dp_20"
                    android:layout_marginTop="@dimen/dp_20"
                    android:id="@+id/mn_car"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:gravity="center_vertical">
                    <ImageView
                        app:tint="@color/black"
                        android:src="@mipmap/ic_mycar"
                        android:layout_width="@dimen/dp_20"
                        android:layout_height="@dimen/dp_20"/>
                    <TextView
                        android:layout_marginTop="@dimen/dp_2"
                        android:textAllCaps="true"
                        android:text="@string/my_cars"
                        android:layout_marginStart="@dimen/dp_10"
                        android:fontFamily="@font/montserrat_semibold"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:textSize="@dimen/sp_16"
                        android:textColor="@color/black"/>
                </androidx.appcompat.widget.LinearLayoutCompat>
                <androidx.appcompat.widget.LinearLayoutCompat
                    android:layout_marginHorizontal="@dimen/dp_20"
                    android:layout_marginTop="@dimen/dp_20"
                    android:id="@+id/mn_inquiry"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:gravity="center_vertical">
                    <ImageView
                        app:tint="@color/black"
                        android:src="@mipmap/ic_myinquiry"
                        android:layout_width="@dimen/dp_20"
                        android:layout_height="@dimen/dp_20"/>
                    <TextView
                        android:layout_marginTop="@dimen/dp_2"
                        android:text="@string/my_inquiries"
                        android:textAllCaps="true"
                        android:layout_marginStart="@dimen/dp_10"
                        android:fontFamily="@font/montserrat_semibold"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:textSize="@dimen/sp_16"
                        android:textColor="@color/black"/>
                </androidx.appcompat.widget.LinearLayoutCompat>
                <androidx.appcompat.widget.LinearLayoutCompat
                    android:layout_marginHorizontal="@dimen/dp_20"
                    android:layout_marginTop="@dimen/dp_20"
                    android:id="@+id/mn_notifi"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:gravity="center_vertical">
                    <ImageView
                        app:tint="@color/black"
                        android:src="@mipmap/ic_bell"
                        android:layout_width="@dimen/dp_20"
                        android:layout_height="@dimen/dp_20"/>
                    <TextView
                        android:layout_marginTop="@dimen/dp_2"
                        android:textAllCaps="true"
                        android:text="@string/notification"
                        android:layout_marginStart="@dimen/dp_10"
                        android:fontFamily="@font/montserrat_semibold"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:textSize="@dimen/sp_16"
                        android:textColor="@color/black"/>
                </androidx.appcompat.widget.LinearLayoutCompat>
                <androidx.appcompat.widget.LinearLayoutCompat
                    android:layout_marginHorizontal="@dimen/dp_20"
                    android:layout_marginTop="@dimen/dp_20"
                    android:id="@+id/mn_mess"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:gravity="center_vertical">
                    <ImageView
                        app:tint="@color/black"
                        android:src="@mipmap/ic_msg"
                        android:layout_width="@dimen/dp_20"
                        android:layout_height="@dimen/dp_20"/>
                    <TextView
                        android:layout_marginTop="@dimen/dp_2"
                        android:textAllCaps="true"
                        android:text="@string/messages"
                        android:layout_marginStart="@dimen/dp_10"
                        android:fontFamily="@font/montserrat_semibold"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:textSize="@dimen/sp_16"
                        android:textColor="@color/black"/>
                </androidx.appcompat.widget.LinearLayoutCompat>
                <androidx.appcompat.widget.LinearLayoutCompat
                    android:layout_marginHorizontal="@dimen/dp_20"
                    android:layout_marginTop="@dimen/dp_20"
                    android:id="@+id/mn_term"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:gravity="center_vertical">
                    <ImageView
                        app:tint="@color/black"
                        android:src="@mipmap/ic_alert"
                        android:layout_width="@dimen/dp_20"
                        android:layout_height="@dimen/dp_20"/>
                    <TextView
                        android:layout_marginTop="@dimen/dp_2"
                        android:textAllCaps="true"
                        android:text="@string/terms"
                        android:layout_marginStart="@dimen/dp_10"
                        android:fontFamily="@font/montserrat_semibold"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:textSize="@dimen/sp_16"
                        android:textColor="@color/black"/>
                </androidx.appcompat.widget.LinearLayoutCompat>
                <androidx.appcompat.widget.LinearLayoutCompat
                    android:layout_marginHorizontal="@dimen/dp_20"
                    android:layout_marginTop="@dimen/dp_20"
                    android:id="@+id/mn_contact"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:gravity="center_vertical">
                    <ImageView
                        app:tint="@color/black"
                        android:src="@mipmap/ic_letter"
                        android:layout_width="@dimen/dp_20"
                        android:layout_height="@dimen/dp_20"/>
                    <TextView
                        android:layout_marginTop="@dimen/dp_2"
                        android:textAllCaps="true"
                        android:text="@string/contact"
                        android:layout_marginStart="@dimen/dp_10"
                        android:fontFamily="@font/montserrat_semibold"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:textSize="@dimen/sp_16"
                        android:textColor="@color/black"/>
                </androidx.appcompat.widget.LinearLayoutCompat>
                <androidx.appcompat.widget.LinearLayoutCompat
                    android:layout_marginHorizontal="@dimen/dp_20"
                    android:layout_marginTop="@dimen/dp_20"
                    android:id="@+id/mn_logout"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:gravity="center_vertical">
                    <ImageView
                        app:tint="@color/black"
                        android:src="@mipmap/ic_logout"
                        android:layout_width="@dimen/dp_20"
                        android:layout_height="@dimen/dp_20"/>
                    <TextView
                        android:layout_marginTop="@dimen/dp_2"
                        android:textAllCaps="true"
                        android:text="@string/logout"
                        android:layout_marginStart="@dimen/dp_10"
                        android:fontFamily="@font/montserrat_semibold"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:textSize="@dimen/sp_16"
                        android:textColor="@color/black"/>
                </androidx.appcompat.widget.LinearLayoutCompat>

            </androidx.appcompat.widget.LinearLayoutCompat>
        </androidx.core.widget.NestedScrollView>
        <androidx.appcompat.widget.LinearLayoutCompat
            android:layout_alignParentBottom="true"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="vertical">
            <View
                android:layout_marginTop="@dimen/dp_20"
                android:layout_width="match_parent"
                android:layout_height="@dimen/dp_1"
                android:background="@color/grey1"/>
            <TextView
                android:text="@string/patent_pending"
                android:layout_gravity="center_horizontal"
                android:layout_marginTop="@dimen/dp_15"
                android:fontFamily="@font/montserrat_semibold"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:textSize="@dimen/sp_12"
                android:textColor="@color/black"/>
            <TextView
                android:layout_gravity="center_horizontal"
                android:id="@+id/tv_app_des"
                android:layout_marginBottom="@dimen/dp_20"
                android:fontFamily="@font/montserrat_medium"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:gravity="center_horizontal"
                android:textSize="@dimen/sp_12"
                android:textColor="@color/grey1"/>
        </androidx.appcompat.widget.LinearLayoutCompat>
    </RelativeLayout>
</androidx.drawerlayout.widget.DrawerLayout>